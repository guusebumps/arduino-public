  [ArduinoInclude( 'Mitov_Packet_Image.h' )]
  [ArduinoClass( 'Mitov::PacketImageBinaryElement' )]
  [CreateName( 'Image' )]
  [Name( 'Image' )]
  [ArduinoOpenWireStudioMatch( TVLImageBufferSplitStructureElement )]
  [CollectionItemAssociation( TVLImageBufferMakeStructureElement )]
  +TArduinoImageBinaryPacketElement : TArduinoBasicPacketElement

    [OWPrimaryPin]
    [OWPinGroup( TArduinoImageBinaryPacketElement )]
    [ArduinoVariableIfPinConnected( 'FPopulated' )]
    InputPin : TOWArduinoImageSinkPin

    [OWExcludeBindings]
    [ArduinoExclude]
    [ArduinoTemplateIfDefault( 'T_CONVERT', '::Mitov::TImageJPEGConvert', '::Mitov::TImageJPEGConvert_Null' )]
    [ArduinoIfNotDefaultInclude( 'Mitov_JPEGDEC.h' )]
    [ArduinoIfNotDefaultInclude( 'Mitov_ImageBuffer_JPEG.h' )]
    IncludeJpeg : Boolean = False
    
  ;
